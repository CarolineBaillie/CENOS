{
  "manifest_version": 2,
  "name": "NotesOrganizer",
  "version": "0.0.1",
  "description": "Description",
  "browser_action": {
      "default_title": "Boilerplate popup!",
      "default_popup": "popup.html"
  },
  "content_scripts": [
      {
          "matches": ["<all_urls>"],
          "js": ["script.js"],
          "css": ["styles.css"]
      }
  ],
  "background": {
      "scripts": ["background.js"],
      "persistent": false
  },
  "permissions": ["tabs", "activeTab", "storage", "<all_urls>"]
}